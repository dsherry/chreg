{
  "name": "Chreg",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Regular expression find/replace",
  "minimum_chrome_version": "16.0.884",
  "permissions": ["tabs"],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [{
    "matches": [ "file://*", "http://*/*", "https://*/*"],
    "js": [ "jquery.min.js", "popup2.js" ],
  }]
}
