{
  "name": "Chreg",
  "version": "0.42",
  "manifest_version": 2,
  "description": "Regular Expression Find and Replace",
  "minimum_chrome_version": "16.0.884",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
   "file://*", "experimental", "tabs", "http://*/*", "https://*/*"
  ],
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": ["jquery.min.js","highlightRegex.min.js"]
    }]
}
